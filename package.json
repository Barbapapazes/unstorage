{
  "name": "unstorage",
  "version": "0.2.0",
  "description": "",
  "repository": "unjs/unstorage",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "./drivers/*": "./drivers/*.js",
    ".": {
      "import": "./dist/unstorage.mjs",
      "require": "./dist/unstorage.js"
    },
    "./server": {
      "import": "./server.mjs",
      "require": "./server.js"
    }
  },
  "main": "./dist/unstorage.js",
  "module": "./dist/unstorage.mjs",
  "types": "./dist/unstorage.d.ts",
  "bin": "./dist/cli.js",
  "files": [
    "dist",
    "drivers",
    "server*"
  ],
  "scripts": {
    "build": "siroc build && mkdist --src src/drivers --dist drivers -d",
    "dev": "jest --watch",
    "demo": "vite demo",
    "lint": "eslint --ext .ts .",
    "prepublishOnly": "yarn build",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish",
    "test": "yarn lint && jest",
    "doctoc": "doctoc README.md --notitle --maxlevel 3",
    "unstorage": "yarn jiti src/cli"
  },
  "dependencies": {
    "anymatch": "^3.1.1",
    "chokidar": "^3.5.1",
    "h3": "^0.2.10",
    "ioredis": "^4.27.3",
    "listhen": "^0.2.4",
    "mri": "^1.1.6",
    "ohmyfetch": "^0.2.0",
    "ufo": "^0.7.5",
    "ws": "^7.4.5"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/ioredis": "latest",
    "@types/jest": "latest",
    "@types/jsdom": "latest",
    "@types/mri": "latest",
    "@types/node": "latest",
    "@types/ws": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vue/compiler-sfc": "latest",
    "destr": "latest",
    "doctoc": "latest",
    "eslint": "latest",
    "jest": "latest",
    "jiti": "latest",
    "jsdom": "latest",
    "mkdist": "latest",
    "monaco-editor": "latest",
    "siroc": "latest",
    "standard-version": "latest",
    "ts-jest": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vue": "^3.0.5"
  }
}
